    *
class Solution:

    def printTriangle(self, N):

        x=1

        for i in range(N,0,-1):

            for j in range(1,x):

                print(end=" ")

            x=x+1

            for k in range(1,2*i):

                print(end="*")

            print()
if __name__ == '__main__':

    t = int(input())

    for _ in range(t):

        N = int(input())

        ob = Solution()

        ob.printTriangle(N)
