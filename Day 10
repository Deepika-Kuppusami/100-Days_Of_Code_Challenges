#Python :

T=int(input())
for i in range (T):
    X,Y=map(int,input().split())
    print((X*10)+(Y*5))

#Java :

import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	Scanner sc = new Scanner(System.in);
	int T=sc.nextInt();
	for(int i=0;i<T;i++)
	{
	int X=sc.nextInt();
	int Y=sc.nextInt();
	System.out.println((X*10)+(Y*5));
	}
	    
	}
}
