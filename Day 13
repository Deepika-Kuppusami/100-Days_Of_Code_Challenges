#Python :

# cook your dish here
T=int(input())
for i in range(T):
    X=int(input())
    if(X%4==0):
        print("Good")
    else:
        print("Not Good")

#Java :

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc= new Scanner(System.in);
		int T=sc.nextInt();
		for (int i=0;i<T;i++)
		{
		    int X=sc.nextInt();
		    if(X%4==0)
		    System.out.println("Good");
		    else
		    System.out.println("Not Good");
		}
	}
}
