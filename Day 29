class Solution:

    def printTriangle(self, N):

        x=1

        for i in range(0,N):

            for j in range(i+1):

                print(x,end=" ")

                x=x+1

            print()
if __name__ == '__main__':

    t = int(input())

    for _ in range(t):

        N = int(input().strip())

        ob = Solution()

        ob.printTriangle(N)

