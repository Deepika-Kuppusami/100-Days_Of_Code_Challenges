class Solution:
    def factorial (self, N):
     
        if(N==0 or N==1):
            return 1
        else:
            return self.factorial(N-)*N

if __name__ == '__main__': 
    t = int (input ())
    for _ in range (t):
        N = int(input())
        ob = Solution()
        if(N==0 or N==1):
        	print(N)
        else:
        	print(ob.factorial(N))
